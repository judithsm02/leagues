{
  "_version": "1.59.0",
  "sap.app": {
    "id": "leagues.leagueslayout",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.0",
      "toolsId": "18379b78-7dab-404e-aec0-79cadb3f4f16"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/service/leagues/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.122.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "leagues.leagueslayout.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "leagues.leagueslayout.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout",
        "bypassed": {
          "target": [
              "leagueslist" ]
      }
    },
      "routes": [
        {
          "name": "leaguesdetails",
          "pattern": "",
          "titleTarget": "",
          "greedy": false,
          "target": [
              "leaguesdetails"
          ]       
        },
        
        { "name":"teamsdetails",
          "pattern": "Teams/{objectId}",
          "titleTarget": "",
          "greedy": false,
          "target": [
              "leaguesdetails",
              "teamsdetails"          ]
        },
        {
          "name": "playersdetails",
          "pattern": "Player/{objectId}/{teams}",
          "titleTarget": "",
          "greedy": false,
          "target": [
              "leaguesdetails",
              "teamsdetails",
              "playersdetails"
          ]
        }
        
      ],
      "targets": {
        "leaguesdetails": {
          "viewType": "XML",
          "transition": "slide",
          "ControlAggregation": "beginColumnPages",
          "viewId": "leagueslist",
          "viewName": "LeaguesList",
          "viewLevel": 1
        },

        "teamsdetails": {
          "viewType": "XML",
          "transition": "slide",
          "ControlAggregation": "midColumnPages",
          "viewId": "teamslist",
          "viewName": "TeamsList",
          "viewLevel": 2
        }, 
        
        "playersdetails": {
          "viewType": "XML",
          "transition": "slide",
          "ControlAggregation": "endColumnPages",
          "viewId": "playerslist",
          "viewName": "PlayersList",
          "viewLevel": 3
        }
      }
    },
    "rootView": {
      "viewName": "leagues.leagueslayout.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
